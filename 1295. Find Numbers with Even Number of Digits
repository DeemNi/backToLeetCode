/**
 * @param {number[]} nums
 * @return {number}
 */
var findNumbers = function (nums) {
    let evenNumbers = 0;
    for (let i = 0; i < nums.length; i++) {
        let count = 0;
        for (let j = 0; j < nums[i].toString().length; j++) {
            count++;
        }
        if(count % 2 == 0) evenNumbers++;
    }

    return evenNumbers;

};
