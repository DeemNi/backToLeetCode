/**
 * @param {number[]} nums
 * @return {number}
 */
var countQuadruplets = function (nums) {
    let distinct = 0;

    for (let i = 0; i < nums.length; i++) {
        for (let j = 0; j < nums.length; j++) {
            for (let k = 0; k < nums.length; k++) {
                for (let l = 0; l < nums.length; l++) {
                    if ((nums[i] + nums[j] + nums[k] == nums[l]) && i < j && j < k && k < l) {
                        distinct++;
                    }
                }
            }
        }
    }

    return distinct;

}; 
